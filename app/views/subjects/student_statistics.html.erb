<div class="container">
<div class="page-header">
	<h3>Статискика студента <%=@stud.name + " " + @stud.surname%> по предмету <%= @subj.title %></h3>
</div>
<div class="col-sm-4">
<table class="table table-striped table-bordered" >	
  <thead>
    <tr>
      <td align="center"><strong>Число</strong></td>
	  <td align="center"><strong>Часов пропущено</strong></td>
    </tr>
  </thead>
  <tbody>
    <% @to_print.each do |dat| %>
      <tr>
        <td width="50%" align="center"><%= dat.date %></td>
        <td width="50%" align="center"><%= dat.hour %></td>
      </tr>
    <% end %>
  </tbody>

</table>
</div>
</div>
